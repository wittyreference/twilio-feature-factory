{
  "name": "@twilio-agent-factory/doc-generator",
  "version": "0.1.0",
  "description": "Auto-generates API documentation and Mermaid diagrams for Twilio Agent Factory",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "clean": "rm -rf dist/"
  },
  "dependencies": {
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
