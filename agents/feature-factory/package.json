{
  "name": "@twilio-feature-factory/feature-factory",
  "version": "0.1.0",
  "description": "Autonomous Twilio feature development orchestrator using Claude Agent SDK",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "feature-factory": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathIgnorePatterns=live-api",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch --testPathIgnorePatterns=live-api",
    "test:live": "NODE_OPTIONS='--experimental-vm-modules' jest __tests__/live-api.test.ts --runInBand",
    "test:all": "NODE_OPTIONS='--experimental-vm-modules' jest --runInBand",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@twilio-feature-factory/mcp-twilio": "^0.1.0",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "glob": "^10.3.0",
    "ora": "^8.0.0",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0",
    "eslint": "^9.39.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wittyreference/twilio-feature-factory.git",
    "directory": "agents/feature-factory"
  },
  "keywords": [
    "twilio",
    "claude",
    "agent",
    "orchestrator",
    "tdd",
    "mcp"
  ],
  "author": "MC",
  "license": "MIT"
}
